    "E,S,M = map(int,input().split())\n",
    "e,s,m = 1,1,1\n",
    "year = 1\n",
    "while True:\n",
    "    if e == E and s == S and m == M:\n",
    "        print(year)\n",
    "        break\n",
    "    e += 1\n",
    "    s += 1\n",
    "    m += 1\n",
    "    if e == 16:\n",
    "        e = 1\n",
    "    if s == 29:\n",
    "        s = 1\n",
    "    if m == 20:\n",
    "        m = 1\n",
    "    year += 1"
